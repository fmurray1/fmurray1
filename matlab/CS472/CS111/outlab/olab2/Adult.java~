/* Adult Class */

public class Adult extends Customer
{

    private String customerNumber, name, address, telephoneNumber, accountNumber;
    private int age, accountType;
    private double balance, checkingInterest=.01, savingsInterest=.0075, checkCharge=.5, overdraftPenalty=35;

    public Adult(String custNum, int a, String n, String ad, String t, int accntType, String acctNum, double bal)
    {
	customerNumber = custNum;
	age = a;
	name = n;
	address = ad;
	telephoneNumber = t;
	accountType = accntType;
	accountNumber = acctNum;
	balance = bal;
    }
    
    public double getCheckCharge()
    {
	return checkCharge;
    }
    
    public double getCheckingInterest()
    {
	return checkingInterest;
    }
    
    public double getOverdraftPenalty()
    {
	return overdraftPenalty;
    }

    public double getSavingsInterest()
    {
	return savingsInterest;
    }
}