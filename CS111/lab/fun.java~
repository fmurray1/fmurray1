/* this is jsut me seeing how to better use this interface */

public class fun
{

    public static void main (String [] args)
    {
	
	System.out.println("               .    .   .              ");
	System.out.println("              / \\  / \\ / \\             ");
	System.out.println("             /   '    '   \\            ");
	System.out.println("            |              |           ");
	System.out.println("             \\____________/           ");
	System.out.println("                  |  |                  ");
	System.out.println("                  |  |                  ");
	System.out.println("                  |  |                  ");
	System.out.println("                . |  |                  ");
	System.out.println("                \\\\|  |                  ");
	System.out.println("                 \\|  |                  ");
	System.out.println("                  |  |                  ");
	System.out.println("                  |  | I love you!!!!   ");
   

	System.out.printf("Hello \n world \n ");




    }
}